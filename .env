# Configuración de Base de Datos PostgreSQL 14
DB_HOST=localhost
DB_PORT=5433
DB_NAME=ucb_admissions
DB_USER=postgres
DB_PASSWORD=admin

# Configuración del Servidor - CAMBIO DE PUERTO
PORT=3000
NODE_ENV=development

# JWT
JWT_SECRET=ucb-super-secret-key-2025-admissions-system
JWT_EXPIRES_IN=24h

# Upload de archivos
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=5000000
ALLOWED_EXTENSIONS=jpg,jpeg,png,pdf

# OCR Configuration
OCR_LANGUAGE=spa
OCR_CONFIDENCE_THRESHOLD=60

# Logging
LOG_LEVEL=info